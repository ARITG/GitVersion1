    <validationRules>
        <fullName>Mobile_First_Flag_Cannot_be_changed</fullName>
        <active>false</active>
        <description>SP are not allowed to edit the Mobile first Flag.</description>
        <errorConditionFormula>OR( IF(AND(ISNEW(),Mobile_First__c = false,OR($Profile.Name = &apos;Sales Professional - Poland&apos;, $Profile.Name =&apos;Sales Professional - Global (US)&apos;)),true,false) ,IF( AND(OR($Profile.Name = &apos;Sales Professional - Poland&apos;, $Profile.Name =&apos;Sales Professional - Global (US)&apos;),ISCHANGED(Mobile_First__c)), true,false))</errorConditionFormula>
        <errorMessage>You do not have permission to edit Mobile First flag.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_Condition</fullName>
        <active>true</active>
        <description>Name can only be Contact</description>
        <errorConditionFormula>AND(OR($Profile.Name = $Label.Sales_Professional_PolandProfile_Name, $Profile.Name = $Label.Sales_Professional_USProfile_Name,$Profile.Name = $Label.Sales_Professional_CPN_India_Profile,$Profile.Name = $Label.Data_Steward_CPN_India, CASESAFEID($User.Id) = $Label.Integrationuserid, $Profile.Name = $Label.Data_Steward_Global_US, $Profile.Name = $Label.Data_Steward_Poland, $Profile.Name = $Label.Data_Steward_Canada, $Profile.Name = $Label.Sales_Professional_CanadaProfile_Name,$Profile.Name = &quot;CFN US Inside Sales DS Force.com&quot;,$Profile.Name = &quot;CFN US Inside Sales Force.com&quot;), NOT((LEFT(WhoId,3) = &apos;003&apos;)),NOT(ISBLANK(WhoId)))</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Please select a Contact record for Name field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Related_To_Condition</fullName>
        <active>false</active>
        <description>Related To can be Account or Opportunity</description>
        <errorConditionFormula>AND(OR($Profile.Name = $Label.Sales_Professional_PolandProfile_Name, $Profile.Name = $Label.Sales_Professional_USProfile_Name, CASESAFEID($User.Id) = $Label.Integrationuserid, $Profile.Name = $Label.Data_Steward_Global_US, $Profile.Name = $Label.Data_Steward_Poland,$Profile.Name = $Label.Data_Steward_Canada, $Profile.Name = $Label.Sales_Professional_CanadaProfile_Name,$Profile.Name = &quot;CFN US Inside Sales DS Force.com&quot;,$Profile.Name = &quot;CFN US Inside Sales Force.com&quot;), NOT(OR(LEFT(WhatId,3) = &apos;001&apos;, LEFT(WhatId,3) = &apos;006&apos;)),NOT(ISBLANK(WhatId)))</errorConditionFormula>
        <errorMessage>Related To can only be either Account or Opportunity</errorMessage>
    </validationRules>
