    <fields>
        <fullName>uf__Activity_Needed__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IMAGE( 
CASE( Rating, 
&quot;Hot&quot;, IF(AND(uf__Days_Since_Activity__c &gt;=3,NOT(ISPICKVAL(Status,&quot;Unqualified&quot;)), NOT(ISPICKVAL(Status,&quot;Unqualified-Spammable&quot;))), &quot;/img/samples/flag_red.gif&quot;, &quot;/s.gif&quot;), 

&quot;Warm&quot;, IF( 

OR(AND(OR(uf__Est_Project_Amount__c &gt;=1000,ISNULL(uf__Est_Project_Amount__c )),uf__Days_Since_Activity__c &gt;=3,NOT(ISPICKVAL(Status,&quot;Unqualified&quot;)), NOT(ISPICKVAL(Status,&quot;Unqualified-Spammable&quot;))) 
,ISNULL(uf__Days_Since_Activity__c)) 
, 

&quot;/img/samples/flag_red.gif&quot;, 

IF( AND(OR(uf__Est_Project_Amount__c &lt;1000,ISNULL(uf__Est_Project_Amount__c )),uf__Days_Since_Activity__c &gt;=5, NOT(ISPICKVAL(Status,&quot;Unqualified&quot;)), NOT(ISPICKVAL(Status,&quot;Unqualified-Spammable&quot;))), 

&quot;/img/samples/flag_yellow.gif&quot;, 

&quot;/s.gif&quot;)), 

&quot;Cold&quot;, IF(ISNULL(uf__Days_Since_Activity__c),&quot;/img/samples/flag_red.gif&quot;, &quot;/s.gif&quot;), 
&quot;&quot;, IF(ISNULL(uf__Days_Since_Activity__c),&quot;/img/samples/flag_red.gif&quot;, &quot;/s.gif&quot;), 

&quot;/s.gif&quot;), 
&quot;Priority Flag&quot;)</formula>
        <inlineHelpText>Does this Lead need to be contacted?  Based on a combination of activity history, qualifications, rating, and project estimate.</inlineHelpText>
        <label>Activity Needed?</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
