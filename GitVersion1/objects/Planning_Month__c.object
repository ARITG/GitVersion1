    <validationRules>
        <fullName>ActivityEndDateLessThanStart</fullName>
        <active>true</active>
        <errorConditionFormula>if(  RecordType.Name=$Label.Cres_Plan_DayRecType &amp;&amp; Plan_Date__c&gt; Activity_End_Date__c,true,false)</errorConditionFormula>
        <errorMessage>Activity End Date should be greater than Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivityEndDateNotAllowed</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(!ISBLANK(Activity_End_Date__c) ,  RecordType.Name ==$Label.Cres_Plan_DayRecType ),if(OR(ISPICKVAL(First_Half__c,&apos;Leave&apos;) &amp;&amp; ISPICKVAL( Second_Half__c,&apos;Leave&apos;),(ISPICKVAL(First_Half__c,&apos;Training&apos;) &amp;&amp; ISPICKVAL(Second_Half__c,&apos;Training&apos;)) 
),false,true),false)</errorConditionFormula>
        <errorMessage>Activity End Date is allowed only if both first half and second half is either Leave/Training</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivityEndDateOutsideMonth</fullName>
        <active>true</active>
        <errorConditionFormula>if(and(!ISBLANK(Activity_End_Date__c ),month(Related_Planning_Month__r.Month_Start_Date__c)!= month( Activity_End_Date__c),RecordType.Name == $Label.Cres_Plan_DayRecType ),true,false)</errorConditionFormula>
        <errorMessage>Activity End Date should be within the planning month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivityEndDateReadOnly</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(RecordType.Name ==$Label.Cres_Plan_DayRecType,ISCHANGED( Activity_End_Date__c ),AND(OR(ISPICKVAL( First_Half__c,&apos;Training&apos;),ISPICKVAL( First_Half__c,&apos;Leave&apos;)),OR(ISPICKVAL( Second_Half__c,&apos;Training&apos;),ISPICKVAL( Second_Half__c,&apos;Leave&apos;)))),true,false)</errorConditionFormula>
        <errorMessage>Activity End Date Cannot be edited for Leave/Training.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivityEndDateRequired</fullName>
        <active>true</active>
        <description>Activity End Date is required if the Activity is Leave/Training</description>
        <errorConditionFormula>if(OR(AND(ISPICKVAL( First_Half__c ,&apos;Leave&apos;),ISPICKVAL( Second_Half__c ,&apos;Leave&apos;)),AND(ISPICKVAL( First_Half__c ,&apos;Training&apos;),ISPICKVAL( Second_Half__c ,&apos;Training&apos;)))&amp;&amp; ISBLANK( Activity_End_Date__c ) &amp;&amp; RecordType.Name ==$Label.Cres_Plan_DayRecType ,true,false)</errorConditionFormula>
        <errorMessage>Activity End Date is mandatory for Leave/Training Activities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivityRequired</fullName>
        <active>true</active>
        <description>First Half and Second Half of the Activity should be selected</description>
        <errorConditionFormula>if((OR(ISBLANK(text(First_Half__c)),ISBLANK( text(Second_Half__c))) &amp;&amp; RecordType.Name ==$Label.Cres_Plan_DayRecType ),TRUE,FALSE)</errorConditionFormula>
        <errorMessage>Please select First Half and Second Half Activities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ApproverMandatory</fullName>
        <active>true</active>
        <description>Status update to Submit for approval will be allowed to change if no of working days in a month equal to no of planned day in a month</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;Submitted for Approval&apos;),OR(
Approver_1__c = NULL, Approver_2__c=NULL),RecordType.Name ==$Label.Cres_PlanMonthRecType)</errorConditionFormula>
        <errorMessage>ASM Notification and RSM Approval should be selected for submission</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Plan_Month</fullName>
        <active>true</active>
        <errorConditionFormula>if(ISNEW() &amp;&amp; RecordType.Name ==$Label.Cres_PlanMonthRecType &amp;&amp; month(today()) != 12, 
AND(Month_Index__c !=month(today()),Month_Index__c !=month(today())+1) || value(text(Plan_Year__c))!=YEAR(today()),
if(ISNEW() &amp;&amp; RecordType.Name ==$Label.Cres_PlanMonthRecType &amp;&amp; month(today()) = 12,if(Month_Index__c=12,value(text(Plan_Year__c))!=YEAR(today()),
if(Month_Index__c=1,value(text(Plan_Year__c))!=YEAR(today())+1,true))

,False) )</errorConditionFormula>
        <errorMessage>Invalid Year or Month Selected. Please select current month or next month for planning.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlanDayCannotBeWeekends</fullName>
        <active>true</active>
        <description>Cannot Create a Plan on Weekends</description>
        <errorConditionFormula>IF(AND( Related_Planning_Month__r.Working_Days_Per_week__c=5,OR(WEEKDAY(Plan_Date__c)=1,WEEKDAY(Plan_Date__c)=7),RecordType.Name ==$Label.Cres_Plan_DayRecType),TRUE,FALSE )</errorConditionFormula>
        <errorMessage>Cannot Create a Plan on Weekends</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlanDayOutsideMonth</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(Month( Related_Planning_Month__r.Month_Start_Date__c)!= month(Plan_Date__c), RecordType.Name==$Label.Cres_Plan_DayRecType ),true,false)</errorConditionFormula>
        <errorMessage>Please select a date within the planning month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlanDayRequired</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(ISBLANK(Plan_Date__c), RecordType.Name==$Label.Cres_Plan_DayRecType ),true,false )</errorConditionFormula>
        <errorMessage>Please select the Day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlanMonthRequired</fullName>
        <active>true</active>
        <errorConditionFormula>if(and(RecordType.Name==&apos;Plan Month&apos;,ISBLANK(text( Month_Planned__c ))),true,false)</errorConditionFormula>
        <errorMessage>Please select the Month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlanYearRequired</fullName>
        <active>true</active>
        <errorConditionFormula>if(and(RecordType.Name==&apos;Plan Month&apos;,ISBLANK(text( Plan_Year__c ))),true,false)</errorConditionFormula>
        <errorMessage>Please select the Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planned_Day_Should_not_be_Sunday</fullName>
        <active>false</active>
        <description>Planned Day should not be a Sunday.</description>
        <errorConditionFormula>IF(And( Related_Planning_Month__r.Working_Days_Per_week__c ==6,WEEKDAY(Plan_Date__c)=1,RecordType.Name ==$Label.Cres_Plan_DayRecType),TRUE,FALSE )</errorConditionFormula>
        <errorMessage>Planned Day Should not be Sunday</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RouteNotAllowed</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(RecordType.Name ==$Label.Cres_Plan_DayRecType,!ISBLANK(Route__c),AND(CONTAINS($Label.Cres_Route_Not_RequiredCPNIndia
,text(First_Half__c)),CONTAINS($Label.Cres_Route_Not_RequiredCPNIndia
,text(Second_Half__c))
)),true,false)</errorConditionFormula>
        <errorMessage>Please do not enter any Route details for selected Activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Route_is_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>if(AND(RecordType.Name ==$Label.Cres_Plan_DayRecType,ISBLANK(Route__c),OR(!CONTAINS($Label.Cres_Route_Not_RequiredCPNIndia
,text(First_Half__c)),!CONTAINS($Label.Cres_Route_Not_RequiredCPNIndia
,text(Second_Half__c))
)),true,false)</errorConditionFormula>
        <errorMessage>Route is Mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TargetRequired</fullName>
        <active>true</active>
        <description>Target is Required for all users</description>
        <errorConditionFormula>if(AND(ISBLANK(Target__c),RecordType.Name == $Label.Cres_PlanMonthRecType ),true,false)</errorConditionFormula>
        <errorMessage>Please select a Territory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WorkingDayValidationForApproval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;Submitted for Approval&apos;),Number_of_Working_Days__c &lt;&gt; Days_Planned__c 
,RecordType.Name ==$Label.Cres_PlanMonthRecType)</errorConditionFormula>
        <errorMessage>Planned Days and Working days  should be same for submission</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
