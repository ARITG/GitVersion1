    <validationRules>
        <fullName>Inform_Supervisor</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Inform_Supervisor__c , NOT(Needs_Manager_Approval__c))</errorConditionFormula>
        <errorMessage>Cannot check Inform Supervisor field if Needs Manager field is not checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rebate_Request_Lock_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Locked__c</errorConditionFormula>
        <errorMessage>Rebate Request Record is Locked. Rebate Request cannot be created, edited, or deleted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_must_be_on_Payer_only</fullName>
        <active>true</active>
        <description>Cannot create a request for an account having another parent account.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(CAR_Account__r.ParentId)),NOT(ISPICKVAL(CAR_Account__r.Parent.Type,&quot;Buying Group&quot;)))</errorConditionFormula>
        <errorDisplayField>CAR_Account__c</errorDisplayField>
        <errorMessage>Rebate Request can be only created for the payer. Your account has parent account. Only parent account is acceptable if it is Buying Group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidFromValidation</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL(Valid_From__c )</errorConditionFormula>
        <errorDisplayField>Valid_From__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
