    <validationRules>
        <fullName>Closed_date_limit</fullName>
        <active>true</active>
        <description>Closed date can&apos;t be earlier than the date the opportunity is created</description>
        <errorConditionFormula>ISNEW() &amp;&amp; CloseDate &lt; Datevalue(CreatedDate)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Estimated Close Date can&apos;t be in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_not_use_Strengthen_stage</fullName>
        <active>true</active>
        <description>This stage cannot be used.</description>
        <errorConditionFormula>ISPICKVAL(StageName,&apos;Strengthen &amp; Expand&apos;) 
&amp;&amp;ISPICKVAL(Business_Unit__c,&apos;Poland&apos;)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Strengthen and Expand stage cannot be used in Poland.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_ID_cannot_be_changed</fullName>
        <active>true</active>
        <description>External Id cannot be changed and should come only by Integration (from external system)</description>
        <errorConditionFormula>IF( 
AND(CONTAINS($User.Profile_Name__c, &quot;Data Steward&quot;), ISCHANGED( External_Id__c )) , true, false)</errorConditionFormula>
        <errorMessage>External Id cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Match_Business_Unit_of_User</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
$Setup.Validation_Rule_Settings__c.Opportunity__c, 
ISNEW(), 
NOT(TEXT( Business_Unit__c ) = TEXT($User.Business_Unit__c)) 
)</errorConditionFormula>
        <errorMessage>Business Unit must match current user&apos;s Business Unit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Polish_Profile_Restriction</fullName>
        <active>false</active>
        <errorConditionFormula>$Profile.Name = &quot;Polish user&quot;</errorConditionFormula>
        <errorDisplayField>Unit_of_Measure_Margin__c</errorDisplayField>
        <errorMessage>Your profile is not allowed to edit this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reminder_frequency_limit</fullName>
        <active>true</active>
        <description>Minimum of 1 day for daily reminders.</description>
        <errorConditionFormula>Days_Between_Email_Alerts__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Days_Between_Email_Alerts__c</errorDisplayField>
        <errorMessage>Lowest value should be 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Won_Lost_PickList_Required</fullName>
        <active>true</active>
        <description>If you selected Won/Lost reason, you must also select if the Opportunity was Won or Lost.</description>
        <errorConditionFormula>(ISBLANK( TEXT(Won_Lost__c) ) 
&amp;&amp; 
OR( 
Credit__c = True, 
Business_Solution__c = True, 
Price__c = True, 
Product_Portfolio__c = True, 
Service__c = True, 
Other_won_lost_reason__c = True 
) 
) 
|| 
( 
