    <validationRules>
        <fullName>New_Route_status_should_be_New_only</fullName>
        <active>true</active>
        <description>New Route records Status should be New only</description>
        <errorConditionFormula>AND( 
$RecordType.Name = &quot;Route&quot;, 
ISNEW(), 
TEXT(Status__c) &lt;&gt; &quot;New&quot; 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status can only be New on creation of new Route</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Route_Unique_Name</fullName>
        <active>false</active>
        <description>AND( RecordType.Name ==&apos;Route&apos;,AND(LOWER(VLOOKUP(($ObjectType.Route__c.Fields.Name), ($ObjectType.Route__c.Fields.Name), (Name))) = LOWER(Name), OR(ISNEW(), ISCHANGED(Name)))) ----- Route Number already exists in the system.</description>
        <errorConditionFormula>AND( RecordType.Name ==&apos;Route&apos;,AND(LOWER(VLOOKUP(($ObjectType.Route__c.Fields.Name), ($ObjectType.Route__c.Fields.Name), (Name))) = LOWER(Name), OR(ISNEW(), ISCHANGED(Name))))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Route Number already exists in the system.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SP_users_select_only_Submit_for_approval</fullName>
        <active>true</active>
        <description>CPN India SP users should select the Submit for approval in route object records</description>
        <errorConditionFormula>AND(
$RecordType.Name = &quot;Route&quot;,
$Profile.Name = &apos;Sales Professional CPN India&apos;,
ISCHANGED ( Status__c ),
$User.Id = OwnerId,
TEXT(Status__c) &lt;&gt; &quot;Submit for Approval&quot;
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Owner should select &quot;Submit for Approval&quot; option from status field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Number_of_customer_Check</fullName>
        <active>true</active>
        <description>Total No. of Customers should be greater than 0 before Submission of Record for Approval.</description>
        <errorConditionFormula>IF( (ISPICKVAL(Status__c,&apos;Submit for Approval&apos;) &amp;&amp; NOT(ISNEW())) &amp;&amp; Total_No_of_Customers_on_Route__c&lt;1, TRUE, FALSE )</errorConditionFormula>
        <errorMessage>Please add at least One Route Customer before Submitting for Approval.</errorMessage>
    </validationRules>
