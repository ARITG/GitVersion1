    <validationRules>
        <fullName>ApplyLogisticDiscountMarkRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNULL( Discount_Percentage__c )), Apply_logistic_discounts__c  = false)</errorConditionFormula>
        <errorDisplayField>Apply_logistic_discounts__c</errorDisplayField>
        <errorMessage>Should be marked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckDiscountsAndValue</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(NOT(ISNULL(Discount_Percentage__c)), 
	OR(NOT(ISNULL(Special_price__c)),
		NOT(ISNULL(Discount_Value__c)))),
		
		
	AND(NOT(ISNULL(Special_price__c)), 
		OR(NOT(ISNULL(Discount_Percentage__c )),
			NOT(ISNULL(Discount_Value__c)))),
		
	AND(NOT(ISNULL(Discount_Value__c )), 
		OR(NOT(ISNULL(Discount_Percentage__c )),
			NOT(ISNULL(Special_price__c))))
			
)</errorConditionFormula>
        <errorMessage>Only one field can be populated for fields Discount %, Discount value and Special price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Product_and_discount_price</fullName>
        <active>false</active>
        <errorConditionFormula>OR(ISBLANK(Product__c),
	AND(ISBLANK( Discount_Percentage__c), 
		ISBLANK(Discount_Value__c),  
		ISBLANK(Special_price__c)
	)
)</errorConditionFormula>
        <errorMessage>You must enter a Product and either a Discount %, Value or Special price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rebate_Line_Lock_Check</fullName>
        <active>true</active>
        <errorConditionFormula>CAR_Rebate_Request__r.Locked__c</errorConditionFormula>
        <errorMessage>Rebate Request Record is Locked. Rebate Lines cannot be created or edited.</errorMessage>
    </validationRules>
